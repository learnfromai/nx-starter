{
  "name": "shared-application",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "shared-application/src",
  "projectType": "library",
  "tags": [],
  "implicitDependencies": ["shared-domain"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/shared-application",
        "main": "shared-application/src/index.ts",
        "tsConfig": "shared-application/tsconfig.lib.json",
        "assets": ["shared-application/*.md"]
      },
      "dependsOn": ["shared-domain:build"]
    }
  }
}
